# Supabase Configuration (auto-populated by Lovable Cloud)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_PUBLISHABLE_KEY=your_supabase_anon_key
VITE_SUPABASE_PROJECT_ID=your_project_id

# Redis (Upstash or managed Redis - required for caching/queues)
VITE_REDIS_URL=rediss://:password@host:port

# RPC Observability Sampling (1.0 = 100%, 0.1 = 10%)
# Lower values reduce DB writes but may miss rare issues
VITE_RPC_OBS_SAMPLE=1.0

# Optional: Error Tracking (Sentry)
VITE_SENTRY_DSN=https://...@sentry.io/...

# Optional: Build Metadata
VITE_COMMIT_SHA=abc123

# Worker (server-side only - DO NOT expose in frontend)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
REDIS_URL=rediss://:password@host:port
JOBS_QUEUE=jobs:main
