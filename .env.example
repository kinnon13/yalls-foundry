# yalls.ai Environment Variables
# Copy this file to .env and fill in your values

# Application
VITE_APP_NAME=yalls.ai
VITE_SITE_URL=https://yalls.ai

# Supabase (Lovable Cloud - auto-configured)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Optional: Upstash Redis for L2 cache
VITE_USE_UPSTASH=false
VITE_UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
VITE_UPSTASH_REDIS_REST_TOKEN=your-token

# Optional: Feature flags (all true in development)
VITE_FEATURE_NEW_SEARCH=false
VITE_FEATURE_ROCKER_AI=false
VITE_FEATURE_VECTOR_SEARCH=false
VITE_FEATURE_ADVANCED_ANALYTICS=false
VITE_FEATURE_EXPERIMENTAL_UI=false